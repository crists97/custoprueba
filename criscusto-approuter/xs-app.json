{
  "authenticationMethod": "none",
  "routes": [
    {
        "source": "^/Northwind/(.*)$",
        "target": "/Northwind/$1",
        "destination": "Northwind",
        "authenticationType": "xsuaa",
        "csrfProtection": false,
        "httpMethods": ["GET", "POST"]
    },{
        "source": "^/getuserinfo/(.*)$",
        "target": "/getuserinfo/$1",
        "destination": "getuserinfo",
        "authenticationType": "xsuaa",
        "csrfProtection": false,
        "httpMethods": ["GET", "POST"]
    }],
  "welcomeFile": "/comcriscustocriscustoapp"
}